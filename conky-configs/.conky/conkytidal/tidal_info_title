#!/bin/bash

first_title=""

	#current_instance=`playerctl metadata -l | grep chromium.instance9`
	current_instance=`playerctl metadata`
	new_title=`playerctl metadata -p $current_instance --format '{{ title }}'`
	if [ "$new_title" != "$first_title" ] 
	then
		first_title="$new_title"
		echo "$new_title"
	fi
	sleep 1

